<%= simple_form_for [cocktail, dose] do |f| %>

  <%= f.association :ingredient %>
  <%= f.input :description %>
  <%= f.submit "Complete Cocktail", class: "btn btn-primary" %>
<% end %>
<%= link_to 'Back to all cocktails', cocktails_path %>

  <li><%= @dose.cocktail %></li>
  <li><%= @dose.ingredient %></li>
